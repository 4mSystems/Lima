<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/nav_profile"
  app:startDestination="@id/profileFragment2">

  <fragment
    android:id="@+id/profileFragment2"
    android:name="app.te.protein_chef.presentation.profile.ProfileFragment"
    android:label="@string/profile">
    <action
      android:id="@+id/actionProfileFragmentToUpdatePassword"
      app:destination="@id/changePasswordFragment2"
      app:enterAnim="@anim/anim_slide_in_right"
      app:exitAnim="@anim/anim_slide_out_left"
      app:popEnterAnim="@anim/anim_slide_in_left"
      app:popExitAnim="@anim/anim_slide_out_right"/>
  </fragment>
  <fragment
    android:id="@+id/changePasswordFragment2"
    android:name="app.te.protein_chef.presentation.profile.changePassword.ChangePasswordFragment"
    android:label="@string/change_password" />
</navigation>